	let p_sig_o = (v_step_o/(2*sqrt(2)))^2
	let snr = p_sig_o/(p_noise_o+p_noise_q)
	let snr_dB = 10*log10(snr)
	print snr_dB
	print onoise_total
	
	setplot tran4
	let vid = v(vid)
	let iop = viop#branch
	let ion = vion#branch
	let vcmo = (voutp+voutn)/2

	*plot iop ion
	plot vid vout vcmo
	plot v(vip) v(vin) v(voutp) v(voutn) vcmo

	*setplot tran5
	*let accuracy_dB = 20*log10(v_step_o/(abs(vgnd)+1p))
	*plot accuracy_dB
end

