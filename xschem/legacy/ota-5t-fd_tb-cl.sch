let v_step_i = v_step_o/Adc

let t_rf = 0.1u
let t_step = 50u
let t_delay = 0
let t_per = 2*t_step

let tstep = 0.1*t_rf
let tstop = 2*t_per
let tstart = t_delay


let fs = 100e6
let t_settle = 1/fs/2
let t_rf_acc = 0.1p
let t_step_acc = 10m
let t_delay_acc = t_settle

let t_per = t_step_acc
let tstep_acc = 0.000001/fs
let tstop_acc = 1p+t_delay_acc
let tstart_acc = t_delay_acc

*alter @VIN[SIN] = [ 0 0.01 $&f_sig 0 0 0 ]
alter @VIN[DC] = 0.0

set wr_singlescale
set wr_vecnames
option numdgt=3

set opSimOnly = 0

** Main Simulations
if $opSimOnly eq 0
	op
	ac dec 100 $&const.f_min $&const.f_max
	*set sqrnoise
	noise v(vout) VIN dec 100 $&const.f_min $&const.f_max 1
	noise v(vout) VIN dec 100 100 5Meg
	alter @VIN[PULSE] = [ 0 $&v_step_i $&t_delay $&t_rf $&t_rf $&t_step $&t_per 0 ]
	** Check power on by ramping Vdd, tie di_pon to VDD net!
	*alter @V1[PULSE] = [ 0 1.8 0 10u $&t_rf 0 0 0 ]
	tran $&tstep $&tstop $&tstart
	*alter @VIN[PULSE] = [ 0 $&v_step_i $&t_delay_acc $&t_rf_acc $&t_rf_acc $&t_step_acc $&t_per_acc 0 ]
	*tran $&tstep_acc $&tstop_acc $&tstart_acc $&tstep_acc
		
